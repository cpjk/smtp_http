<h2>Show email</h2>

<ul>

  <li>
    <strong>Data:</strong>
    <%= @email.data %>
  </li>

  <li>
    <strong>Sender email:</strong>
    <%= @email.sender_email %>
  </li>

  <li>
    <strong>Receiver email:</strong>
    <%= @email.receiver_email %>
  </li>

</ul>

<%= link "Edit", to: email_path(@conn, :edit, @email) %>
<%= link "Back", to: email_path(@conn, :index) %>
